{
  "name": "grafana-bigquery-datasource",
  "version": "1.4.1",
  "description": "Google BigQuery datasource for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:sign": "yarn run build && yarn run sign",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "lint:ts": "eslint . --ext .js,.tsx,.ts --cache",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:codecov": "jest --config jest.config.js && codecov",
    "test:jest": "jest --config jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "10.1.5",
    "@grafana/experimental": "1.7.3",
    "@grafana/google-sdk": "0.1.2",
    "@grafana/runtime": "10.1.5",
    "@grafana/ui": "10.1.5",
    "@react-awesome-query-builder/ui": "^6.4.2",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^17.4.3",
    "react-virtualized-auto-sizer": "^1.0.20",
    "sql-formatter-plus": "^1.3.6",
    "tslib": "2.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "1.2.0-rc1",
    "@swc/core": "^1.3.103",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "^6.31.3",
    "css-loader": "^6.9.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "10.3.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.2",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@1.22.19"
}
